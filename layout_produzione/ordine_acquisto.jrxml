	<queryString>
		<![CDATA[WITH
  "location" AS (
    SELECT
      loc.c_location_id,
      ctrl.ad_language,
      loc.address1 || COALESCE(' ' || loc.address2,'') AS address,
      COALESCE(loc.city,'') AS city,
      COALESCE(loc.postal,'') AS postal,
      COALESCE(ctrl.name,cou.name,'') AS country,
      COALESCE(cou.countrycode,'') AS countrycode,
      COALESCE(reg.name,'') AS regioncode,
      COALESCE(reg.description,'') AS region,
      COALESCE(cou.ad_language, cli.ad_language, 'en_EN') AS country_lang
    FROM c_location loc
    LEFT JOIN c_region reg ON reg.c_region_id = loc.c_region_id
    LEFT JOIN c_country cou ON cou.c_country_id = loc.c_country_id
    LEFT JOIN c_country_trl ctrl ON ctrl.c_country_id = cou.c_country_id
    LEFT JOIN ad_client cli ON cli.ad_client_id = loc.ad_client_id)
SELECT
  -- Organization info
  o.ad_org_id as org_id,
  COALESCE(org.social_name,org.name) as org_name,
  oloc.address as org_address,
  oloc.city as org_city,
  oloc.postal as org_postal,
  oloc.country as org_country,
  oloc.countrycode as org_countrycode,
  oloc.regioncode as org_regioncode,
  oloc.region as org_region,
  orginfo.taxid as org_iva,
  --ELIMINATO--orginfo.em_opfatt_headingline_a as org_heading1,
  --ELIMINATO--orginfo.em_opfatt_headingline_b as org_heading2,
  --ELIMINATO--orginfo.em_opfatt_headingline_c as org_heading3,
  --ELIMINATO--orginfo.em_base_cellulare as contact,

  -- Document info
  o.dateordered as ord_date,
  to_char(o.dateordered) as ord_datestr,
  o.documentno as ord_docno,
  o.description as ord_desc,
  o.poreference as ord_poreference,
  project.value as commessa,
  o.freightcostrule as porto,
  o.em_opregiv_cig as cig,
  o.em_opregiv_cup as cup,
  o.poreference as offerta,
  COALESCE(paytermtrl.name,payterm.name) as ord_paymentterm,
  finacc.name as ord_bankname,
  finacc.iban as ord_iban,
  ROUND(discount.discount,2) as ord_discount,
  ROUND(o.totallines,2) as ord_netamt,
  ROUND(o.grandtotal,2) as ord_grossamt,
  -- -ROUND(o.totalpaid,2) as ord_totalpaid,
  --ROUND(o.outstandingamt,2) as ord_outstandingamt,
  curr.cursymbol as ord_currencysymb,
  curr.iso_code as ord_currency,

  -- Customer info
  bp.name as bp_name,
  bp.name2 as bp_cf,
  bp.value as bp_code,
  bp.fiscalcode as bp_fiscalcode,
  bp.taxid as bp_taxid,
  bloc.address as bp_address,
  bloc.city as bp_city,
  bloc.postal as bp_postal,
  bloc.country as bp_country,
  bloc.regioncode as bp_regioncode,
  bloc.region as bp_region,

  -- Destination info
  case when o.em_bau_other_dest='Y' then bpdest.name else COALESCE(org.social_name,org.name) end as dest_name,
  case when o.em_bau_other_dest='Y' then blocdest.address else  oloc.address end as dest_address,
  case when o.em_bau_other_dest='Y' then blocdest.city else oloc.city end as dest_city,
  case when o.em_bau_other_dest='Y' then blocdest.postal else oloc.postal end as dest_postal,
  case when o.em_bau_other_dest='Y' then blocdest.country else oloc.country end as  dest_country,
  case when o.em_bau_other_dest='Y' then blocdest.regioncode else oloc.regioncode  end as  dest_regioncode,
  case when o.em_bau_other_dest='Y' then blocdest.region else oloc.region end as  dest_region,


 -- trasportatore info
  m_shipper.name as shpname,
  vet.name as vet_name,
  vet.value as vet_code,
  vet.fiscalcode as vet_fiscalcode,
  vet.taxid as vet_taxid,
  vetbloc.address as vet_address,
  vetbloc.city as vet_city,
  vetbloc.postal as vet_postal,
  vetbloc.country as vet_country,
  vetbloc.regioncode as vet_regioncode,
  vetbloc.region as vet_region,

  -- Lines
  ol.qtyordered as li_qty,
  COALESCE(uomtrl.name, uom.name) as li_udm,
  uom.uomsymbol as li_udmsymbol,
  ol.linenetamt as li_netamt,
  ol.priceactual as li_unitprice,
  ol.discount as li_discount,
  p.value as li_prodcode,
  p.name as li_prodname,
 -- ol.account_id as li_speseacc,
  --ol.financial_invoice_line as li_fil,
  ol.description as li_proddesc,
  ol.datepromised as li_dataconsegna,
  tax.name as li_taxname,
  ROUND(tax.rate/100*ol.linenetamt,2) as li_taxamt,
  row_number() OVER (ORDER BY ol.line ASC, ol.c_orderline_id ASC) as li_lineno,
  --ELIMINATO--o.em_har_nrcolli as nr_colli,

  --ELIMINATO--o.em_har_datainiziotrasp,
  --ELIMINATO--aspetto.name as aspetto,
  o.deliveryviarule as consegna,
  o.datepromised as dataconsegna,
  o.em_bau_frase as frase,
COALESCE(COALESCE(C_DOCTYPE_TRL.PRINTNAME,C_DOCTYPE.PRINTNAME)||' - ','P.L.')||COALESCE(o.DOCUMENTNO||' - ','')||COALESCE(to_Char(to_date(to_Char(o.dateordered),'dd/MM/yyyy')),'') AS docTitle
FROM c_order as o

-- Customer info
INNER JOIN c_bpartner bp ON bp.c_bpartner_id = o.c_bpartner_id
INNER JOIN c_bpartner_location bploc ON bploc.c_bpartner_location_id = o.c_bpartner_location_id
INNER JOIN "location" bloc ON (bloc.c_location_id = bploc.c_location_id AND
                               bloc.ad_language = bloc.country_lang)

--Destinazione BP info
LEFT JOIN c_bpartner_location destloc ON destloc.c_bpartner_location_id = o.c_bpartner_location_id
LEFT JOIN "location" destbloc ON (destbloc.c_location_id = destloc.c_location_id AND
                               destbloc.ad_language = destbloc.country_lang)

-- Destinazione Merce info
LEFT JOIN c_bpartner bpdest ON bpdest.c_bpartner_id = o.em_bau_destinat_id
LEFT JOIN c_bpartner_location bplocdest ON bplocdest.c_bpartner_location_id = o.em_bau_destinaz_id
LEFT JOIN "location" blocdest ON (blocdest.c_location_id = bplocdest.c_location_id AND
                               blocdest.ad_language = blocdest.country_lang)

--trasportatore info
LEFT JOIN m_shipper  ON m_shipper.m_shipper_id = o.m_shipper_id
LEFT JOIN c_bpartner vet ON vet.c_bpartner_id = m_shipper.c_bpartner_id
LEFT JOIN c_bpartner_location vetloc ON vetloc.c_bpartner_id = vet.c_bpartner_id
LEFT JOIN "location" vetbloc ON (vetbloc.c_location_id = vetloc.c_location_id AND
                               vetbloc.ad_language = vetbloc.country_lang)


-- Organization info
INNER JOIN ad_org org ON org.ad_org_id = o.ad_org_id
LEFT  JOIN ad_orginfo orginfo ON orginfo.ad_org_id = o.ad_org_id
LEFT  JOIN "location" oloc ON (oloc.c_location_id = orginfo.c_location_id AND
                               oloc.ad_language = bloc.country_lang)
-- Document info
LEFT  JOIN c_paymentterm payterm ON payterm.c_paymentterm_id = o.c_paymentterm_id
LEFT  JOIN c_paymentterm_trl paytermtrl ON (paytermtrl.c_paymentterm_id = payterm.c_paymentterm_id AND
                                            paytermtrl.ad_language = bloc.country_lang)
LEFT  JOIN FIN_Financial_Account finacc ON finacc.FIN_Financial_Account_ID = bp.PO_Financial_Account_ID
LEFT  JOIN c_currency curr ON curr.c_currency_id = o.c_currency_id
LEFT  JOIN c_project project ON o.c_project_id = project.c_project_id
LEFT  JOIN c_order_discount orderdiscount ON o.c_order_id = orderdiscount.c_order_id
LEFT  JOIN c_discount discount ON orderdiscount.c_discount_id = discount.c_discount_id

-- Lines
INNER JOIN c_orderline ol ON ol.c_order_id = o.c_order_id
left JOIN m_product p ON p.m_product_id = ol.m_product_id
INNER JOIN c_tax tax ON tax.c_tax_id = ol.c_tax_id
INNER JOIN c_uom uom ON uom.c_uom_id = ol.c_uom_id
LEFT  JOIN c_uom_trl uomtrl ON (uomtrl.c_uom_id = uom.c_uom_id AND
                                uomtrl.ad_language = bloc.country_lang)
--ELIMINATO--left join opddt_aspetto as aspetto on (aspetto.opddt_aspetto_id = o.em_har_aspetto_id)
LEFT JOIN C_DOCTYPE ON o.C_DOCTYPE_ID = C_DOCTYPE.C_DOCTYPE_ID
	LEFT JOIN C_DOCTYPE_TRL ON C_DOCTYPE.C_DOCTYPE_ID = C_DOCTYPE_TRL.C_DOCTYPE_ID AND C_DOCTYPE_TRL.AD_LANGUAGE = $P{LANGUAGE}

WHERE o.c_order_id = $P{DOCUMENT_ID}
ORDER BY ol.line ASC, ol.c_orderline_id ASC
-- WHERE o.documentno ='1']]>
	</queryString>
