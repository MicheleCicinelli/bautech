	<queryString>
		<![CDATA[WITH
  "location" AS (
    SELECT
      loc.c_location_id,
      ctrl.ad_language,
      loc.address1 || COALESCE(' ' || loc.address2,'') AS address,
      COALESCE(loc.city,'') AS city,
      COALESCE(loc.postal,'') AS postal,
      COALESCE(ctrl.name,cou.name,'') AS country,
      COALESCE(cou.countrycode,'') AS countrycode,
      COALESCE(reg.name,'') AS regioncode,
      COALESCE(reg.description,'') AS region,
      COALESCE(cou.ad_language, cli.ad_language, 'en_EN') AS country_lang
    FROM c_location loc
    LEFT JOIN c_region reg ON reg.c_region_id = loc.c_region_id
    LEFT JOIN c_country cou ON cou.c_country_id = loc.c_country_id
    LEFT JOIN c_country_trl ctrl ON ctrl.c_country_id = cou.c_country_id
    LEFT JOIN ad_client cli ON cli.ad_client_id = loc.ad_client_id)
SELECT
  -- Organization info
  m.ad_org_id as org_id,
 orginfo.taxid as org_iva,
  COALESCE(org.social_name,org.name) as org_name,
  oloc.address as org_address,
  oloc.city as org_city,
  oloc.postal as org_postal,
  oloc.country as org_country,
  oloc.countrycode as org_countrycode,
  oloc.regioncode as org_regioncode,
  oloc.region as org_region,
  --orginfo.em_opfatt_headingline_a as org_heading1,
  --orginfo.em_opfatt_headingline_b as org_heading2,
  --orginfo.em_opfatt_headingline_c as org_heading3,
  --orginfo.em_base_cellulare as contact,
  -- Document info
  m.movementdate as inv_date,
  to_char( m.movementdate) as inv_datestr,
  m.documentno as inv_docno,
  m.description as inv_desc,
  ord.c_order_id,
  ord.documentno as orderdocno,
  ord.dateordered as dateordered,
  m.deliveryviarule as metodo,
  --m.poreference as inv_poreference,
  m.freightcostrule as porto,
  --COALESCE(paytermtrl.name,payterm.name) as inv_paymentterm,
  --fp.name as inv_paymentmethod,
  --finacc.name as inv_bankname,
  --finacc.iban as inv_iban,
  --ROUND(i.totallines,2) as inv_netamt,
  --ROUND(i.grandtotal,2) as inv_grossamt,
  --ROUND(i.totalpaid,2) as inv_totalpaid,
  --ROUND(i.outstandingamt,2) as inv_outstandingamt,
  --curr.cursymbol as inv_currencysymb,
  --curr.iso_code as inv_currency,
  -- Customer info
  bp.name as bp_name,
  bp.name2 as bp_cf,
  bp.value as bp_code,
  bp.fiscalcode as bp_fiscalcode,
  bp.taxid as bp_taxid,
  bloc.address as bp_address,
  bloc.city as bp_city,
  bloc.postal as bp_postal,
  bloc.country as bp_country,
  bloc.regioncode as bp_regioncode,
  bloc.region as bp_region,

  -- Destination info

  case when destbloc.address is not null then destbloc.address else  bloc.address end as dest_address,
  case when destbloc.city  is not null then destbloc.city else bloc.city end as dest_city,
  case when destbloc.postal  is not null then destbloc.postal else bloc.postal end as dest_postal,
  case when destbloc.country  is not null then destbloc.country else bloc.country end as  dest_country,
  case when destbloc.regioncode   is not null then destbloc.regioncode else bloc.regioncode  end as  dest_regioncode,
  case when destbloc.region   is not null then destbloc.region else bloc.region end as  dest_region,


 -- trasportatore info
  m_shipper.name as shpname,
  vet.name as vet_name,
  vet.value as vet_code,
  vet.fiscalcode as vet_fiscalcode,
  vet.taxid as vet_taxid,
  vetbloc.address as vet_address,
  vetbloc.city as vet_city,
  vetbloc.postal as vet_postal,
  vetbloc.country as vet_country,
  vetbloc.regioncode as vet_regioncode,
  vetbloc.region as vet_region,

  -- Lines
  ml.movementqty as li_qty,
  COALESCE(uomtrl.name, uom.name) as li_udm,
 -- ol.linenetamt as li_netamt,
 -- ol.em_har_discount as li_sconto,
  --il.priceactual as li_unitprice,
  p.value as li_prodcode,
  p.name as li_prodname,
 -- il.account_id as li_speseacc,
--  il.financial_invoice_line as li_fil,
  ml.description as li_proddesc,
 -- tax.name as li_taxname,
  --ROUND(tax.rate/100*il.linenetamt,2) as li_taxamt,
  row_number() OVER (ORDER BY ml.line ASC, ml.m_inoutline_id ASC) as li_lineno,
  m.nopackages as nr_colli,
  shipdate,
  cu.name as causale,
  --aspetto.name as aspetto,
  --causale.name as causale,
 m.deliveryviarule as consegna,
 ga.description as aspetto,
 m.em_isd_weight as peso,
 pr.value as commessa



FROM m_inout m

-- Customer info
INNER JOIN c_bpartner bp ON bp.c_bpartner_id = m.c_bpartner_id
INNER JOIN c_bpartner_location bploc ON bploc.c_bpartner_location_id = m.c_bpartner_location_id
INNER JOIN "location" bloc ON (bloc.c_location_id = bploc.c_location_id)


--Destinazione info
LEFT JOIN c_bpartner_location destloc ON destloc.c_bpartner_location_id = m.delivery_location_id
LEFT JOIN "location" destbloc ON (destbloc.c_location_id = destloc.c_location_id )

--trasportatore info
LEFT JOIN m_shipper  ON m_shipper.m_shipper_id = m.m_shipper_id
LEFT JOIN c_bpartner vet ON vet.c_bpartner_id = m_shipper.c_bpartner_id
LEFT JOIN c_bpartner_location vetloc ON vetloc.c_bpartner_id = vet.c_bpartner_id
LEFT JOIN "location" vetbloc ON (vetbloc.c_location_id = vetloc.c_location_id)


-- Organization info
INNER JOIN ad_org org ON org.ad_org_id = m.ad_org_id
LEFT  JOIN ad_orginfo orginfo ON orginfo.ad_org_id = m.ad_org_id
LEFT  JOIN "location" oloc ON (oloc.c_location_id = orginfo.c_location_id)
-- Document info
--LEFT  JOIN c_paymentterm payterm ON payterm.c_paymentterm_id = i.c_paymentterm_id
--LEFT  JOIN c_paymentterm_trl paytermtrl ON (paytermtrl.c_paymentterm_id = payterm.c_paymentterm_id AND
                                            --paytermtrl.ad_language = bloc.country_lang)
--LEFT  JOIN FIN_Financial_Account finacc ON finacc.FIN_Financial_Account_ID = bp.FIN_Financial_Account_ID
--LEFT  JOIN c_currency curr ON curr.c_currency_id = i.c_currency_id
-- Lines
INNER JOIN m_inoutline ml ON ml.m_inout_id = m.m_inout_id
left JOIN m_product p ON p.m_product_id = ml.m_product_id
--INNER JOIN c_tax tax ON tax.c_tax_id = ml.c_tax_id
INNER JOIN c_uom uom ON uom.c_uom_id = ml.c_uom_id
LEFT  JOIN c_uom_trl uomtrl ON (uomtrl.c_uom_id = uom.c_uom_id AND
                                uomtrl.ad_language = bloc.country_lang)
--left join opddt_aspetto as aspetto on (aspetto.opddt_aspetto_id = m.em_opddt_aspetto_id)
--left join opddt_causale as causale on (causale.opddt_causale_id = m.em_opddt_causale_id)
LEFT JOIN isd_goodsaspect ga ON m.em_isd_goodsaspect_id = ga.isd_goodsaspect_id
LEFT JOIN c_project pr ON m.c_project_id = pr.c_project_id
left join bau_causale as cu on m.em_bau_causale_id =  cu.bau_causale_id


--Ordine info
LEFT JOIN c_orderline ordl ON (ml.c_orderline_id = ordl.c_orderline_id)
LEFT JOIN c_order ord ON (ordl.c_order_id = ord.c_order_id)


--LEFT JOIN fin_paymentmethod fp ON i.fin_paymentmethod_id = fp.fin_paymentmethod_id
--WHERE m.documentno =''
WHERE m.m_inout_id = $P{DOCUMENT_ID}
ORDER BY ord.c_order_id, ml.line ASC, ml.m_inoutline_id ASC]]>
	</queryString>
